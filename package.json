{
  "name": "ng2-preybird",
  "version": "0.1.1",
  "scripts": {
    "start": "concurrently \"npm run css\" \"npm run scss:w\" \"npm run tsc:w\" \"npm run lite\" ",
    "css": "gulp sass",
    "scss:w": "gulp sass:watch",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "live-server --open=unit-tests.html",
    "e2e-test": "concurrently \"npm run webdriver\" \"npm run protractor\"",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "protractor": "protractor protractor.conf.js",
    "webdriver": "webdriver-manager start && webdriver-manager start"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "^2.0.0-beta.12",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.24",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "gulp-sass": "^2.2.0",
    "jasmine-core": "2.4.1",
    "lite-server": "^2.1.0",
    "live-server": "^0.9.2",
    "node-sass": "^3.4.2",
    "protractor": "^3.2.2",
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  }
}
